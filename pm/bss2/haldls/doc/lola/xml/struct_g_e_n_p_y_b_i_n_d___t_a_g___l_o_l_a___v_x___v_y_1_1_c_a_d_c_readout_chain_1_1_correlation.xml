      <memberdef kind="variable" id="struct_g_e_n_p_y_b_i_n_d___t_a_g___l_o_l_a___v_x___v_y_1_1_c_a_d_c_readout_chain_1_1_correlation_1abf2082c62f3604044639de0885433742" prot="public" static="no" mutable="no">
